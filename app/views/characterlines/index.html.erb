<p id="notice"><%= notice %></p>

<h1>Characterlines</h1>

<table>
  <thead>
    <tr>
      <th>Character</th>
      <th>Line</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @characterlines.each do |characterline| %>
      <tr>
        <td><%= characterline.character %></td>
        <td><%= characterline.line %></td>
        <td><%= characterline.project_id %></td>
        <td><%= link_to 'Show', characterline %></td>
        <td><%= link_to 'Edit', edit_characterline_path(characterline) %></td>
        <td><%= link_to 'Destroy', characterline, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Characterline', new_characterline_path %>
