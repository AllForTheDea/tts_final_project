<% if @project != nil %>
  <% @project.characterlines.each do |line| %>
	  <div class="container-fluid" style = "border-color: black;">
		<div class="well" style = "border-color: black;">
		   <span style = "font-weight: bold;"><%= line.character %>:</span> <%= line.line %> <br /><br />
		   <div style = "width:100%; height:2px; background: black; border-radius: 75%;"></div><br />
		   <div class="btn btn-danger">
			Record PH
			</div>	
		</div>
	   </div>
	   <br />
  <% end %>



<% end %>




